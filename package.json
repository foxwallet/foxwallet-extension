{
  "name": "foxwallet-extension",
  "version": "0.0.6",
  "description": "FoxWallet chrome extension (In development)",
  "type": "module",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "vite --mode development",
    "build:dev": "npm run clean && node ./scripts/predev.js && yarn build:dapp &&  vite build --mode development && cd offscreen_transaction && NODE_ENV=development npx rollup -c && cd .. && cp ./offscreen_tx.html ./dist/offscreen_tx.html",
    "build:prod": "npm run clean && yarn wasm && node ./scripts/prerelease.js && yarn build:dapp && vite build && cd offscreen_transaction && NODE_ENV=production npx rollup -c && cd .. && cp ./offscreen_tx.html ./dist/offscreen_tx.html && cp ./dist/assets/aleo_wasm_bg.wasm ./dist/aleo_wasm_bg.wasm",
    "build:dapp": "vite build --config vite-injector.config.ts",
    "clean": "rimraf dist/ && rimraf node_modules/aleo_wasm/",
    "wasm": "cd @aleohq/aleo_wasm && RUSTFLAGS='-C link-arg=--max-memory=4294967296' wasm-pack build --release --target web",
    "translate": "node ./scripts/translate.cjs",
    "lint": "eslint --ext .ts,.js --max-warnings=0 .",
    "format": "prettier --write .",
    "postinstall": "node ./scripts/postinstall.cjs"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@aleohq/wasm": "file:./@aleohq/wasm",

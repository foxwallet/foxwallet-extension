{
  "name": "@provablehq/wasm",
  "version": "0.8.0",
  "description": "SnarkVM WASM binaries with javascript bindings",
  "collaborators": [
    "The Provable Team"
  ],
  "license": "GPL-3.0",
  "type": "module",
  "main": "./dist/mainnet/index.js",
  "browser": "./dist/mainnet/index.js",
  "types": "./dist/mainnet/index.d.ts",
  "exports": {
    ".": "./dist/mainnet/index.js",
    "./worker.js": "./dist/testnet/worker.js",
    "./testnet.js": "./dist/testnet/index.js",
    "./testnet/worker.js": "./dist/testnet/worker.js",
    "./mainnet.js": "./dist/mainnet/index.js",
    "./mainnet/worker.js": "./dist/mainnet/worker.js"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProvableHQ/sdk.git"
  },
  "keywords": [
    "Aleo",
    "Blockchain",
    "Zero-Knowledge",
    "ZK"
  ],
  "bugs": {
    "url": "https://github.com/ProvableHQ/sdk/issues"
  },
  "homepage": "https://github.com/ProvableHQ/sdk#readme",
  "scripts": {
    "build:mainnet": "yarn clean && node scripts/wasmpack.js mainnet",
    "build:testnet": "yarn clean && node scripts/wasmpack.js testnet",
    "build:networks": "yarn build:mainnet && yarn build:testnet",
    "build:wasm": "yarn clean && rimraf dist && node build.js && node ./scripts/copywasm.js",
    "build:all": "yarn build:networks && yarn build:wasm",
    "clean": "rimraf pkg && rimraf target",
    "test": "node test.js"
  },
  "devDependencies": {
    "@rollup/plugin-virtual": "^3.0.2",
    "@wasm-tool/rollup-plugin-rust": "^3.0.4",
    "binaryen": "^121.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.32.0"
  }
}
